{
  "name": "@aws/fhir-works-on-aws-routing",
  "version": "6.6.2",
  "description": "FHIR Works on AWS routing implementation",
  "homepage": "https://github.com/aws-solutions/fhir-works-on-aws/fwoa-core/routing",
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/fhir-works-on-aws-routing.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "email": "fhir-works-on-aws-dev@amazon.com",
    "url": "https://aws.amazon.com/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "heft build --clean && rushx pkg-json-lint",
    "build-test": "heft test --clean && rushx pkg-json-lint",
    "clean": "rm -rf build/* node_modules/* dist/* .serverless/* .nyc_output/* lib/*",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx",
    "local": "node .",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "release": "rush build && rush lint-fix && rush test",
    "sort-package-json": "sort-package-json package.json",
    "standard-version": "standard-version --skip.tag=true",
    "test": "heft test --clean --no-build && rushx pkg-json-lint",
    "watch": "tsc -w"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".test.ts",
    "transform": {
      "\\.(ts)$": "ts-jest"
    }
  },
  "resolutions": {
    "json5": "^2.2.3",
    "path-parse": "^1.0.7",
    "qs": "^6.10.3",
    "set-value": "^4.0.1"
  },
  "dependencies": {
    "@aws/fhir-works-on-aws-interface": "workspace:*",
    "@types/cors": "^2.8.7",
    "@types/express-serve-static-core": "^4.17.2",
    "ajv": "^6.11.0",
    "ajv-errors": "^1.0.1",
    "aws-sdk": "^2.1000.0",
    "aws-xray-sdk": "^3.3.3",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "flat": "^5.0.2",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.26",
    "sanitize-html": "^2.8.0",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws/eslint-config-fwoa-eslint-custom": "workspace:*",
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "0.49.0",
    "@rushstack/heft-jest-plugin": "0.4.2",
    "@rushstack/heft-node-rig": "1.11.11",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.2",
    "@types/flat": "^5.0.1",
    "@types/heft-jest": "^1.0.3",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.182",
    "@types/mime-types": "^2.1.0",
    "@types/mock-req-res": "^1.1.2",
    "@types/node": "^14",
    "@types/sanitize-html": "^2.8.0",
    "@types/uuid": "^8.3.4",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^8.7.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-mock-extended": "^1.0.8",
    "mock-req-res": "^1.2.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.4.1",
    "sinon": "^11.0.0",
    "sort-package-json": "^1.57.0",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
