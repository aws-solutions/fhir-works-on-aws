  "main": "src/index.ts",
  "scripts": {
    "build": "heft build --clean && rushx pkg-json-lint",
    "build-test": "heft test --clean && rushx pkg-json-lint",
    "cdk": "cdk",
    "cdk-deploy": "rush build && cdk deploy --all --require-approval never --outputs-file Info_Output.log",
    "clean": "rm -rf build/* node_modules/* dist/* .nyc_output/*",
    "compile-igs": "ts-node scripts/compile-igs.ts",
    "deploy": "cdk deploy --outputs-file Info_Output.log",
    "int-test": "jest --testPathPattern=src/integration-tests/* --runInBand",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx",
    "local": "node .",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json . --i ../../.npmpackagejsonlintignore",
    "release": "rush build && rush lint-fix && rush test",
    "sort-package-json": "sort-package-json package.json",
    "test": "heft test --clean --no-build 2>&1 && rushx pkg-json-lint",
    "test-coverage": "jest --coverage --testPathPattern=src/*",
    "watch": "tsc -w"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": ".test.ts",
    "testRunner": "jest-circus/runner",
    "transform": {
      "\\.(ts)$": "ts-jest"
    }
  },
  "resolutions": {
