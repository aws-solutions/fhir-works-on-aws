{
  "name": "@aws/fhir-works-on-aws-search-es",
  "version": "3.12.1",
  "description": "FHIR Works on AWS search Elasticsearch implementation",
  "homepage": "https://github.com/aws-solutions/fhir-works-on-aws/fwoa-core/search-es",
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/fhir-works-on-aws-search-es.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "email": "fhir-works-on-aws-dev@amazon.com",
    "url": "https://aws.amazon.com/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "heft build --clean && rushx pkg-json-lint",
    "build-test": "heft test --clean && rushx pkg-json-lint",
    "clean": "rm -rf build/* node_modules/* dist/* .serverless/* .nyc_output/* lib/*",
    "jest": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx",
    "local": "node .",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "prepublish": "tsc",
    "release": "yarn run build && yarn run lint && yarn run test",
    "sort-package-json": "sort-package-json package.json",
    "standard-version": "standard-version --skip.tag=true",
    "test": "TZ=UTC heft test --clean --no-build && rushx pkg-json-lint",
    "test-coverage": "jest --coverage",
    "watch": "tsc -w"
  },
  "jest": {
    "collectCoverage": true
  },
  "resolutions": {
    "json5": "2.2.3",
    "set-value": "^4.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.50.0",
    "@aws-sdk/node-http-handler": "^3.52.0",
    "@aws/fhir-works-on-aws-interface": "workspace:*",
    "@elastic/elasticsearch": "7.13.0",
    "@types/aws-lambda": "^8.10.92",
    "aws-elasticsearch-connector": "^8.2.0",
    "aws-sdk": "^2.1000.0",
    "aws-xray-sdk": "^3.3.3",
    "date-fns": "^2.19.0",
    "flat": "^5.0.2",
    "lodash": "^4.17.20",
    "nearley": "^2.20.0",
    "qs": "^6.10.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws/eslint-config-fwoa-eslint-custom": "workspace:*",
    "@elastic/elasticsearch-mock": "^0.3.0",
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "0.49.0",
    "@rushstack/heft-jest-plugin": "0.4.2",
    "@rushstack/heft-node-rig": "1.11.11",
    "@types/flat": "^5.0.1",
    "@types/heft-jest": "^1.0.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.161",
    "@types/nearley": "^2.11.1",
    "@types/node": "^14",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.7.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-mock-extended": "^1.0.8",
    "json-stringify-pretty-compact": "^2.0.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.4.1",
    "sort-package-json": "^1.57.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
